{
    "name": "codegea/codegea",
    "description": "The Codegea Framework.",
    "keywords": ["framework", "codegea"],
    "license": "MIT",
    "type": "project",
    "authors": [
        {
            "name": "Derian Pinto",
            "email": "pintoderian@gmail.com",
            "homepage": "https://codegea.com"
        }
    ],
    "require": {
        "illuminate/database": "^5.6.12",
        "twig/twig": "^2.4.7",
        "bramus/router": "^1.3",
        "opis/http": "^2.0"
    },
    "autoload": {
        "classmap": [
            "core\\helpers\\Extension.php",
            "core\\BaseController.php"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
