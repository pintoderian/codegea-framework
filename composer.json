{
    "name": "codegea/codegea",
    "description": "The Codegea Framework.",
    "keywords": ["framework", "codegea"],
    "license": "MIT",
    "type": "project",
    "authors": [
        {
            "name": "Derian Pinto",
            "email": "pintoderian@gmail.com",
            "homepage": "https://codegea.com"
        }
    ],
    "require": {
        "bramus/router": "^1.3",
        "filp/whoops": "^2.1",
        "illuminate/database": "^5.6.12",
        "illuminate/pagination": "^5.6",
        "symfony/http-foundation": "^4.1",
        "symfony/security-csrf": "^4.1",
        "symfony/var-dumper": "^4.1",
        "twig/twig": "^2.4"
    },
    "autoload": {
        "classmap": [
            "core\\helpers\\Extension.php",
            "core\\Loader.php"
        ],
        "files": ["core/helpers/Functions.php"]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
